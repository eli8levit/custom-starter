    "configstore": "^4.0.0",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "inquirer": "^6.2.2",
    "ora": "^3.1.0"
  },
  "devDependencies": {
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  }
}
