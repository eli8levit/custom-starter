{
  "name": "custom-starter",
  "version": "1.0.0",
  "description": "Start you projects fast simply cloning already created boilerplate repos",
  "repository": {
    "type": "git",
    "url": "https://github.com/jediyozh/custom-starter"
  },
  "license": "MIT",
  "author": {
    "name": "Eli Levit",
    "email": "jediyozh@gmail.com"
  },
  "main": "bin/index.js",
